{
    "name": "mikecheng/pay",
    "description": "Alipay、WeChat、Palpay和Stripe 的支付扩展包",
    "keywords": ["alipay", "wechat", "Palpay", "Stripe", "pay"],
    "type": "library",
    "support": {
        "issues": "https://github.com/mikecheng/pay/issues",
        "source": "https://github.com/mikecheng/pay"
    },
    "authors": [
        {
            "name": "mikecheng",
            "email": "ianmikecheng@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.3",
        "ext-openssl": "*",
        "ext-simplexml":"*",
        "ext-libxml": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.2",
        "yansongda/pay": "^2.8",
        "symfony/http-foundation": "^4.0",
        "symfony/event-dispatcher": "^4.0"
    },
    "require-dev": {
        "predis/predis": "^1.1",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Mikecheng\\Pay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mikecheng\\Pay\\Tests\\": "tests"
        }
    }
}
